<?xml version="1.0" encoding="UTF-8"?>
<tns:lwiConfiguration xmlns:tns="http://telekom.hu/LwiConfiguration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://telekom.hu/LwiConfiguration LwiConfiguration.xsd ">
  <tns:log>
    <tns:detail>FULL</tns:detail>
  </tns:log>
  <tns:consumers>
    <tns:consumerApplication>
      <tns:name>tns:name</tns:name>
      <tns:password>tns:password</tns:password>
    </tns:consumerApplication>
  </tns:consumers>
  <tns:providers>
    <tns:provider>
      <tns:name>tns:name</tns:name>
      <tns:loadBalancerHost>tns:loadBalancerHost</tns:loadBalancerHost>
      <tns:loadBalancerPort>0</tns:loadBalancerPort>
      <tns:protocol>http</tns:protocol>
      <tns:services>
        <tns:service>
          <tns:name>tns:name</tns:name>
          <tns:businessService>tns:businessService</tns:businessService>
          <tns:validationType>CTX</tns:validationType>
          <tns:maxRequests>0</tns:maxRequests>
          <tns:requestQueueLength>0</tns:requestQueueLength>
          <tns:allowedConsumers>
            <tns:consumerName>tns:consumerName</tns:consumerName>
          </tns:allowedConsumers>
        </tns:service>
      </tns:services>
    </tns:provider>
  </tns:providers>
</tns:lwiConfiguration>
